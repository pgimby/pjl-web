  <body onload=initRepoPage()>



    <!-- HEADER FOR STYLE - MAYBE THROW SOME NAVIGATION IN HERE EVENTUALLY -->

    <header>
      <div id="header-baseline">
      <div class="wrap">
        <a id="pjl-logo" href="/">PJL</a>
        <div class="need-help">
          <i class="fa fa-bug fa-lg" aria-hidden="true"></i>
          <p class="need-help-text">report bug</p>
        </div>
      </div>
      </div>

    </header>







    <!-- MAIN PAGE CONTENT -->


    <main>

      <div class="wrap">


        <!-- THE HERO CONTAINS THE TITLE ELEMENTS -->

        <div class="hero">
          <div class="title-flex">
            <h1 id="title">Department of Physics and Astronomy</h1>
            <h1 id="subtitle">Physics Junior Laboratory Document Repository</h1>
          </div>
        </div>





        <!-- SEARCH BAR WITH SEARCH HELP DROPDOWN -->

        <h2>Find labs by search term</h2>

        <div class="search-container">
        <form class="search-box">
          <input type="text" id="search-bar" placeholder="Keyword, topic, course..." onfocus="this.placeholder = ''" onblur="this.placeholder = 'Keyword, topic, course...'">
          <a><img class="search-icon" src="/img/search-icon.png"></a> <!-- may I see your silly walk? -->
          <p id="search-help-button">search tips</p>
          <div id="search-help">
            <p id="tips">Tips</p>
            <p>Searches try to match against all lab record meta data</p>
            <p>Prefix your search terms with <span class="code">meta:</span> to match against only that identifier</p>
            <div class="example"><p id="help-ex">Ex.</p><p><span class="code">course:PHYS 325</span> will match the query <span class="code">PHYS 325</span> against only course names</p>
              <span class="code">id:</span>
              <span class="code">course:</span>
              <span class="code">lab:</span>
              <span class="code">year:</span>
              <span class="code">semester:</span>
              <span class="code">topic:</span>
              <span class="code">discipline:</span>
              <span class="code">equipment:</span>
              <span class="code">eqid:</span>
            </div>
          </div>
        </form>
        </div>



        <span>
          <h2 id="or">OR</h2>
          <h2>Find labs by filtering the database</h2>
        </span>







        <!-- LAB RECORDS FILTER SELECTIONS - FILLED DYNAMICALLY -->

        <div class="filter">
          <div id="course-filter">
            <h3>Course</h3>
            <select id="course-select" multiple="multiple">
            </select>
          </div>
          <div id="year-filter">
            <h3>Year</h3>
            <select id="year-select" multiple="multiple">
            </select>
          </div>
          <div id="semester-filter">
            <h3>Semester</h3>
            <select id="semester-select" multiple="multiple">
            </select>
          </div>
          <div id="discipline-filter">
            <h3>Discipline</h3>
            <select id="discipline-select" multiple="multiple">
            </select>
          </div>
          <div id="topic-filter">
            <h3>Topic</h3>
            <select id="topic-select" multiple="multiple">
            </select>
          </div>
          <div id="filter-mod-buttons">
            <p id="clear-filters-button">clear filters</p>
            <p id="show-recent-button">most recent</p>
          </div>
        </div>




        <!-- SEARCH RESULTS COUNTER WITH ZIP ICON -->


        <p id="search-results">Showing <span id="num-unmasked-results"></span> of <span id="num-results"></span> search results</p>
        <div id="zip-icon"></div>
        <p id="expand-all-button" expanded="false">expand all</p>





        <!-- LAB RECORDS DISPLAY AND HEADER FOR SORTING FUNCTIONALITY -->


        <div id="records-header">
            <a style="visibility: hidden; margin: 0 11px">Download</a> <!-- just here to match spacing on records display -->
            <p class="record-header-button" id="sort-course" sorted="false"><span>Course</span></p>
            <p class="record-header-button" id="sort-version">
              <span class="sort-span-mobile" id="sort-semester" sorted="false">Sem.</span>
              <span class="sort-span-desktop" id="sort-semester" sorted="false">Semester</span>
              /
              <span id="sort-year" sorted="false">Year</span>
            </p>
            <p class="record-header-button" id="sort-name" sorted="false"><span>Lab</span></p>
            <div style="width:46px"></div>
          </div>

        <div id="record-list-box">

        </div>
        <p id="show-all-button">show all</p></br>













      </div>  <!-- close main wrap -->

    </main>







